//Bruteforce Approach

class Solution {
    public String defangIPaddr(String address) {
        String[]arr=address.split("\\.");
        StringBuilder res=new StringBuilder();

        for(int i=0;i<address.length-1;i++)
        {
          res.append(arr[i]);
          res.append("[.]");
        }

        res.append(address[address.length-1]);
        
      return res.toString();
    }
}


//Optimal Approach

class Solution {
    public String defangIPaddr(String address) {
        return address.replace(".", "[.]");
    }
}
